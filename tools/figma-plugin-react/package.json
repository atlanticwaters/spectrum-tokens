{
  "name": "@adobe/spectrum-figma-plugin-react",
  "version": "0.2.0",
  "description": "Figma plugin to export and sync design tokens with Adobe Spectrum (React version)",
  "keywords": [
    "figma",
    "plugin",
    "design-tokens",
    "spectrum",
    "adobe",
    "design-system",
    "react"
  ],
  "type": "module",
  "main": "./dist/code.js",
  "scripts": {
    "build": "node build.js",
    "watch": "node build.js --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"./src\" \"./test\"",
    "lint": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=10.17.1"
  },
  "packageManager": "pnpm@10.17.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/spectrum-tokens.git",
    "directory": "tools/figma-plugin-react"
  },
  "author": "Adobe Design System Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@reduxjs/toolkit": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.102.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.5",
    "typescript": "^5.7.2"
  }
}
